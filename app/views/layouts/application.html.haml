!!! 5
%html
  %head
    %title Ichthys-Basar
    = stylesheet_link_tag 'application'
    = javascript_include_tag 'application'
    = csrf_meta_tags

  %body
    %table#menu
      %tr
        %td= link_to "Sellers", sellers_path
        %td
          = link_to "Transactions", transactions_path
          = link_to "(all)", all_transactions_path

    - if flash[:alert]
      #alert.message= flash[:alert]
    - if flash[:error]
      #error.message= flash[:error]
    - if flash[:warning]
      #warning.message= flash[:warning]
    - if flash[:notice]
      #notice.message= flash[:notice]

    = yield
